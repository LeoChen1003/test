INSERT INTO dept VALUES(40,'测试部');
CREATE TABLE emp(
	eid INT PRIMARY KEY AUTO_INCREMENT,
	ename VARCHAR(4),
	sex VARCHAR(1),
	birthday DATE,
	salary INT,
	deptId INT,
	FOREIGN KEY(deptId) REFERENCES dept(did)
);
